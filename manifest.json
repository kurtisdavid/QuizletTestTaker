{
  "manifest_version": 2,

  "name": "QuizletTestTaker",
  "description": "Take quizlet tests without clicking",
  "version": "1.0",
  "background": {
    "service_worker": "popup.js"
  },
  "content_scripts": [{
    "js": ["jquery.min.js"],
    "matches": ["http://*/*", "https://*/*"],
    "run_at": "document_start"
 }],
 "commands": {
    "inject-script": {
      "suggested_key": "1",
      "description": "injecting script"
    }
  },
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}